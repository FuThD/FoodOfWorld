	<dict>
		<key>albums</key>
		<array>
			<string>http://img.juhe.cn/cookbook/t/7/6648_893430.jpg</string>
		</array>
		<key>burden</key>
		<string>盐,适量;姜,适量;葱,适量;绍酒,100g</string>
		<key>id</key>
		<string>6648</string>
		<key>imtro</key>
